{
    "name": "jbalbert",
    "description": "This is a boiler plate for Slim 3 that includes Twig, Flash messages, Monolog, forrest and swagger.",
    "authors": [
        {
            "name": "JB Albert",
            "email": "jbalbertjoligon@gmail.com"
        }
    ],
    "require": {
        "slim/slim": "^3.0",    
        "slim/twig-view": "^2.0",    
        "slim/flash": "^0.1.0",
        "swiftmailer/swiftmailer": "^5.4",
        "monolog/monolog": "^1.13",
        "namshi/jose": "^7.1",
        "zircote/swagger-php": "^2.0",
        "phpseclib/phpseclib": "^2.0",
        "illuminate/database": "*",
        "catfan/medoo": "*",
        "mpdf/mpdf": "^7.1.0",
        "wisembly/elephant.io": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Module\\": "module"
        },
        "files": [
            "tools/include.php"
        ]
    }
}
